    "coverage": "nyc -r text -e .ts -x \"src/tests/*.test.ts\" npm run test"
  },
  "author": "Melle Hofman <melle@capone.nl>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@types/sinon": "^9.0.10",
    "chai": "^4.2.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:melletron/typescript-mutex-decorator.git"
  }
